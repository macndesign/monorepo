{
  "name": "@macndesign/monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@material-ui/core": "^4.11.3",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webpack": "^4.41.26",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "jest": "^26.6.3",
    "jest-haste-map": "^26.6.2",
    "jest-resolve": "^26.6.2",
    "jest-styled-components": "^7.0.3",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.1",
    "styled-components": "^5.2.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "type-check": "tsc --noEmit --jsx react-jsx",
    "build": "lerna exec --parallel -- babel --root-mode upward src -d lib --extensions \".js,.ts,.tsx\" --source-maps --ignore **/*.stories.js,**/*.stories.tsx,**/*.spec.js,**/*.spec.tsx",
    "build:ts": "lerna run build --stream",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "dev": "start-storybook -p 5555",
    "coverage": "jest --coverage",
    "unit": "jest"
  }
}
